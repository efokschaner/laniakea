{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/demos/pong/server/dist/index.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}/demos/pong/server",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/packages/**/dist/**/*.js", "${workspaceRoot}/demos/**/dist/**/*.js"]
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": false,
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/packages/**/dist/**/*.js", "${workspaceRoot}/demos/**/dist/**/*.js"],
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    },
    {
      "name": "Debug Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/tests/network-integration-test/dist/index.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}/tests/network-integration-test",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/packages/**/dist/**/*.js", "${workspaceRoot}/tests/**/dist/**/*.js"]
    },
  ]
}
